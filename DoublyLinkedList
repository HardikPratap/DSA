//Basic structure of doubly linked list

public class DLL {
    //there are two ways of defining Doubly Linked List 
    //1.With Tail
    //2.without Tail
    //i have used without tail method in this program
    
    Node head;
    private class Node{
        int val;
        Node next;
        Node prev;

        public Node(int val) {
            this.val = val;
        }

        public Node(int val, Node next, Node prev) {
            this.val = val;
            this.next = next;
            this.prev = prev;
        }
}
